PREFIX gps: <http://josd.github.io/fluid/gps/gps-schema#>
PREFIX : <http://josd.github.io/fluid#>
PREFIX math: <http://www.w3.org/2000/10/swap/math#>
PREFIX ex: <http://example.org#>


# The strange values in the list:
#	
# ?MaxDuration ?MaxCost ?MinSuccessRate ?MinComfort ?MaxStagecount	
#

{
	?SCOPE gps:findpath (
			{
				?SUBJECT :location :Oostende
			} 
			?PATH 
			?DURATION 
			?COST 
			?BELIEF 
			?COMFORT (3600.0 5.0 0.2 0.4 1)
		).
} => {
	?SUBJECT ex:hasPath ( ?PATH ?DURATION ?COST ?COMFORT ) .
}.